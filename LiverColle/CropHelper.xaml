        mc:Ignorable="d"
        Title="CropHelper" Topmost="True" AllowsTransparency="True" WindowStyle="None" WindowState="Maximized" Background="#7F9B9B9B" MouseLeftButtonDown="Window_MouseLeftButtonDown" MouseLeftButtonUp="Window_MouseLeftButtonUp" MouseMove="Window_MouseMove" KeyDown="Window_KeyDown">
